type User {
  id: ID! @id
  name: String!
  email: String!
  password: String!
  created_at: DateTime! @createdAt
  updated_at: DateTime! @updatedAt
  objectives: [Objective!]!
}

type Objective {
  id: ID! @id
  name: String!
  description: String!
  user_id: User @relation(link: INLINE)
  status: Boolean! default(value: false)
  created_at: DateTime! @createdAt
  updated_at: DateTime! @updatedAt

}

